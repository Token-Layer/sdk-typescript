{
  "name": "@token-layer/sdk-typescript",
  "version": "0.1.5",
  "description": "TypeScript SDK for Token Layer /token-layer endpoint",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Token-Layer/sdk-typescript.git"
  },
  "homepage": "https://github.com/Token-Layer/sdk-typescript#readme",
  "bugs": {
    "url": "https://github.com/Token-Layer/sdk-typescript/issues"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run generate:openapi-types && tsc -p tsconfig.json",
    "changeset": "changeset",
    "clean": "rm -rf dist",
    "example:create-token": "npm run build && tsx examples/create-token.ts",
    "example:create-token:builder-code": "npm run build && tsx examples/create-token-builder-code.ts",
    "example:get-tokens-v2": "npm run build && tsx examples/get-tokens-v2.ts",
    "example:get-tokens-v2:builder-code": "npm run build && tsx examples/get-tokens-v2-builder-code.ts",
    "example:action:trade-token": "npm run build && tsx examples/action-trade-token.ts",
    "example:action:send-transaction": "npm run build && tsx examples/action-send-transaction.ts",
    "example:action:transfer-token": "npm run build && tsx examples/action-transfer-token.ts",
    "example:action:claim-rewards": "npm run build && tsx examples/action-claim-rewards.ts",
    "example:action:create-referral-code": "npm run build && tsx examples/action-create-referral-code.ts",
    "example:action:enter-referral-code": "npm run build && tsx examples/action-enter-referral-code.ts",
    "example:action:mint-usd": "npm run build && tsx examples/action-mint-usd.ts",
    "example:info:me": "npm run build && tsx examples/info-me.ts",
    "example:info:get-pool-data": "npm run build && tsx examples/info-get-pool-data.ts",
    "example:info:get-user-balance": "npm run build && tsx examples/info-get-user-balance.ts",
    "example:info:search-token": "npm run build && tsx examples/info-search-token.ts",
    "example:info:check-token-ownership": "npm run build && tsx examples/info-check-token-ownership.ts",
    "example:info:get-user-fees": "npm run build && tsx examples/info-get-user-fees.ts",
    "example:info:get-user-fee-history": "npm run build && tsx examples/info-get-user-fee-history.ts",
    "example:info:get-leaderboard": "npm run build && tsx examples/info-get-leaderboard.ts",
    "example:info:get-user-portfolio": "npm run build && tsx examples/info-get-user-portfolio.ts",
    "generate:openapi-types": "npm run generate:token-layer-openapi && openapi-typescript src/generated/token-layer-openapi.yaml -o src/generated/openapi.ts",
    "generate:token-layer-openapi": "node scripts/extract-token-layer-openapi.mjs",
    "version-packages": "changeset version"
  },
  "keywords": [
    "token-layer",
    "sdk",
    "typescript",
    "viem",
    "supabase"
  ],
  "license": "MIT",
  "dependencies": {
    "viem": "^2.41.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@types/node": "^25.3.0",
    "dotenv": "^17.3.1",
    "openapi-typescript": "^7.13.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "yaml": "^2.8.1"
  }
}
